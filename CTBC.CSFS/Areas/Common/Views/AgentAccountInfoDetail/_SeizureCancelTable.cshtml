
@model IList<CTBC.CSFS.Models.CaseSeizure>
@{
    Layout = null;
}

<div class="row">
    <div class="col-xs-12">
        <div class="box box-primary">
            <div class="box-header">
                <i class="glyphicon glyphicon-th"></i>
                <h3 class="box-title">@Lang.csfs_result_area</h3>
            </div><!-- /.box-header -->
            <div id="" class="box-body">
                <table id="" class="table table-bordered table-striped text-black">
                    <thead>
                    <tr class="alert-success text-center">
                        <th class="col-xs-1">@Lang.csfs_selectd</th>
                        <th class="col-xs-1">@Lang.csfs_case_no</th>
                        <th class="col-xs-1">@Lang.csfs_id_1</th>
                        <th class="col-xs-1">@Lang.csfs_name</th>
                        <th class="col-xs-1">@Lang.csfs_case_unit</th>
                        <th class="col-xs-1">@Lang.csfs_bank_name</th>
                        <th class="col-xs-1">@Lang.csfs_deposit_account</th>
                        <th class="col-xs-1">@Lang.csfs_currency_1</th>
                        <th class="col-xs-1">@Lang.csfs_balance</th>
                        <th class="col-xs-1">@Lang.csfs_cal_caseamount</th>
                    </tr>
                    </thead>
                    <tbody>
                    @if (Model != null && Model.Any())
                    {
                        for (int i = 0; i < Model.Count; i++)
                        {
                            <tr>
                                <td class="text-center">
                                    <input type="checkbox" class="checkboxCancel" value="@Model[i].SeizureId" />
                                </td>
                                <td>@Model[i].CaseNo</td>
                                <td>@Model[i].CustId</td>
                                <td>@Model[i].CustName</td>
                                <td>@Model[i].BranchNo</td>
                                <td>@Model[i].BranchName</td>
                                <td>@Model[i].Account</td>
                                <td>@Model[i].Currency</td>
                                <td>@Model[i].Balance</td>
                                <td>@Model[i].SeizureAmount</td>
                            </tr>
                        }
                    }
                    else
                    {
                        <tr class="gridrow"><td colspan="10"><span class="text-red text-center">@Lang.csfs_query_nodata</span></td></tr>
                    }
                    </tbody>
                </table>
            </div>
            <div class="box-footer text-center">
                <button id="btnSaveSeizureCancel" type="button" class="btn btn-primary btn-xs">@Lang.csfs_save</button>
            </div>
        </div>
    </div>
</div>